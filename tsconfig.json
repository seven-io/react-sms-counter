{
    "compilerOptions": {
    "baseUrl": ".",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "jsx": "preserve",
        "lib": [
        "DOM",
        "ES2017"
    ],
        "module": "CommonJS",
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": true,
        "target": "ES2016"
},
    "exclude": [
    "node_modules"
]
}